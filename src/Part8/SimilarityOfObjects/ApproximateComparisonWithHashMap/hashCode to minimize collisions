String
public int hashCode() {
    int hash = 31;
    for (int i = 0; i < length(); i++) {
        hash = hash * 31 + charAt(i);
    }
    return hash;
}

Integer
public int hashCode() {
    int hash = 31;
    for (int i = 0; i < length(); i++) {
        hash = hash * 31 + charAt(i);
    }
    return hash;
}

